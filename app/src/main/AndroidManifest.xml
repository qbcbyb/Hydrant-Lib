<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.qbcbyb.lib">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="cn.qbcbyb.lib.AppCustom"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MetroActivity"
            android:label="MetroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cn.qbcbyb.lib.MainActivity3"
            android:label="@string/title_activity_main_activity3"></activity>
        <activity
            android:name="cn.qbcbyb.lib.MainActivity"
            android:label="@string/title_activity_main"></activity>
        <activity
            android:name="cn.qbcbyb.lib.MainActivity2"
            android:label="@string/title_activity_main_activity2"></activity>
    </application>

</manifest>
